<div class="container">
  <div class="row">
    <div class="new-movie-select">
      <div class="col-xs-9 md-6">
      <h4>What movie would you like to review?</h4>

      <%= form_for(@movie) do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
</div>

